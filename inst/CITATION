citHeader("To cite PARTITIONR in publictions use:")

citEntry(entry       = "Unpublished",
         title       = paste({PARTITIONR}: R package for partitioning species",
                             "diversity across spatial scales."),
         author      = personList(as.person("Michael Mahon"),
                                  as.person("Michael Cunningham-Minnick"),
                                  as.person("Thomas Crist"),
                                  as.person("Hannah Penn"),
                                  as.person("Joseph Veech"),
                                  as.person("Keith Summerville")),
         year        = "2019",
         note        = "R package version 0.0.1",
         textVersion = paste("Michael Mahon, Michael Cunningham-Minnick, Thomas Crist",
                             "Hannah Penn, Joseph Veech, and Keith Summerville (2019).",
                             {PARTITIONR}: R package for partitioning species",
                             "diversity across spatial scales."
                             "R package version 0.0.1."))
